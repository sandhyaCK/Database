total_number_of_outgoing_packets_which_were_dropped ,
boot_time ,
keyboard, 
mouse,
technology ,
files_changed) values(10001,
101,
now(),
new.Cpu_Count,Cpu_Working_Time ,
new.cpu_percent,
new.Usage_cpu_count,
new.number_of_software_interrupts_since_boot,
new.number_of_system_calls_since_boot,
